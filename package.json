{
  "name": "cylinderx-api",
  "version": "1.0.0",
  "description": "CylinderX API - A comprehensive TypeScript Node.js Express MySQL application with JWT authentication",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon",
    "build": "rm -rf dist && tsc",
    "start": "pnpm db:migrate && pnpm db:seed && TS_NODE_PROJECT=tsconfig.prod.json node -r tsconfig-paths/register dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "db:create": "ts-node src/scripts/createDatabase.ts",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:seed": "sequelize-cli db:seed:all",
    "db:migration:generate": "sequelize-cli migration:generate",
    "db:launch": "bash db.sh",
    "db:clear-sessions": "ts-node src/scripts/clear-login-sessions.ts",
    "docs:new-feature": "bash scripts/create-feature-doc.sh"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "express",
    "mysql",
    "sequelize",
    "jwt",
    "boilerplate"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "20",
    "pnpm": ">=6.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.832.0",
    "@aws-sdk/client-ses": "^3.830.0",
    "@sendgrid/mail": "^8.1.5",
    "@types/bull": "^4.10.4",
    "@types/cookie-parser": "^1.4.9",
    "@types/node-cron": "^3.0.11",
    "@types/qrcode": "^1.5.5",
    "add": "^2.0.6",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "file-type": "16.5.4",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "json2csv": "6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^3.0.1",
    "moment": "^2.30.1",
    "multer": "^2.0.1",
    "mysql2": "^3.14.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.3",
    "pnpm": "^10.12.4",
    "qrcode": "^1.5.4",
    "redis": "^5.5.6",
    "redis-json": "^6.0.3",
    "resend": "^4.6.0",
    "sequelize": "^6.37.7",
    "sharp": "^0.34.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/json2csv": "^5.0.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mime-types": "^3.0.1",
    "@types/multer": "^1.4.13",
    "@types/node": "^24.0.3",
    "@types/nodemailer": "^6.4.17",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "jest": "^30.0.2",
    "nodemon": "^3.1.10",
    "openapi-types": "^12.1.3",
    "prettier": "^3.5.3",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@8.15.5+sha512.b051a32c7e695833b84926d3b29b8cca57254b589f0649d899c6e9d0edb670b91ec7e2a43459bae73759bb5ce619c3266f116bf931ce22d1ef1759a7e45aa96f"
}
